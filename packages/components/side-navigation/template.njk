<div class="nhsuk-side-navigation-options {%- if params.classes %} {{ params.classes }}{% endif %}"
{%- for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %}>
  <ul class="nhsuk-side-navigation__list">
    {%- for item in params.items %}
      <li class="nhsuk-side-navigation__item">
        <a class="nhsuk-side-navigation__link {% if item.active %} nhsuk-side-navigation__current {% endif %}" href="#{{ item.id }}">{{ item.display }}</a>
      </li>
    {%- endfor %}
  </ul>
</div>
<div class="nhsuk-side-navigation-spacer"></div>
